{
    "name": "peergum/geodb",
    "description": "Provides Laravel with geographical databases and APIs",
    "keywords": [
        "laravel",
        "countries",
        "cities",
        "states",
        "API"
    ],
    "authors": [
        {
            "name": "Phil Hilger",
            "email": "phil@peergum.com",
            "role" : "Developer"
        }
    ],
    "license": "Apache-2.0",
    "require": {
        "php": "^8.2",
        "laravel/framework": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Peergum\\GeoDB\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Peergum\\GeoDB\\Providers\\GeoDBProvider"
            ]
        }
    },
    "minimum-stability" : "stable",
    "prefer-stable" : true
}
